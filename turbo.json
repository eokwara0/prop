{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": [
    "NEST_API_URL"

  ],
  "globalDependencies": ["**/.env.*local", "**/.env"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["NEST_API_URL","NEXT_PUBLIC_AZURE_MAPS_KEY"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {},
    "test": {},
    "test:e2e": {}
  }
}
