  "components": {
    "schemas": {
      "SignInData": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "AuthToken": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string"
          }
        },
        "required": [
          "access_token"
        ]
      },
      "SignupDto": {
        "type": "object",
        "properties": {}
      },
      "IGetSession": {
        "type": "object",
        "properties": {
          "sessionToken": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "expires": {
            "type": "string"
          }
        },
        "required": [
          "sessionToken",
          "userId",
          "expires"
        ]
      },
      "GetSessionRole": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "id": {
            "type": "number"
          },
          "createdAt": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "description",
          "id",
          "createdAt"
        ]
      },
      "GetSessionUserWithRoles": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "emailVerified": {
            "format": "date-time",
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GetSessionRole"
            }
          }
        },
        "required": [
          "id",
          "email",
          "emailVerified",
          "name",
          "image",
          "roles"
        ]
      },
      "GetSessionAndUserResult": {
        "type": "object",
        "properties": {
          "session": {
            "$ref": "#/components/schemas/IGetSession"
          },
          "user": {
            "$ref": "#/components/schemas/GetSessionUserWithRoles"
          }
        },
        "required": [
          "session",
          "user"
        ]
      },
      "CreatePropertyDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "house",
              "apartment",
              "townhouse",
              "condo",
              "duplex",
              "commercial",
              "land"
            ]
          },
          "name": {
            "type": "string"
          },
          "streetName": {
            "type": "string"
          },
          "streetNumber": {
            "type": "number"
          },
          "suburb": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "bedrooms": {
            "type": "number"
          },
          "bathrooms": {
            "type": "number"
          },
          "hasParking": {
            "type": "boolean"
          },
          "isFurnished": {
            "type": "boolean"
          },
          "ownerId": {
            "type": "string"
          },
          "isForRent": {
            "type": "boolean"
          },
          "isForSale": {
            "type": "boolean"
          },
          "images": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "isActive": {
            "type": "boolean"
          },
          "description": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "postalCode": {
            "type": "string"
          },
          "squareFeet": {
            "type": "number"
          },
          "price": {
            "type": "number"
          },
          "mainImage": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "name",
          "streetName",
          "streetNumber",
          "suburb",
          "country",
          "bedrooms",
          "bathrooms",
          "hasParking",
          "isFurnished",
          "ownerId",
          "isForRent",
          "isForSale",
          "images",
          "isActive"
        ]
      }
