  "tags": [
    {
      "name": "Domio",
      "description": ""
    }
  ],
  "servers": [],
  "components": {
    "schemas": {
      "SignInData": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "AuthToken": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string"
          }
        },
        "required": [
          "access_token"
        ]
      },
      "SignupDto": {
        "type": "object",
        "properties": {}
      },
      "IGetSession": {
        "type": "object",
        "properties": {
          "sessionToken": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "expires": {
            "type": "string"
          }
        },
        "required": [
          "sessionToken",
          "userId",
          "expires"
        ]
      },
      "GetSessionRole": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "id": {
            "type": "number"
          },
          "createdAt": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "description",
          "id",
          "createdAt"
        ]
      },
      "GetSessionUserWithRoles": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "emailVerified": {
            "format": "date-time",
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GetSessionRole"
            }
          }
        },
        "required": [
          "id",
          "email",
          "emailVerified",
          "name",
          "image",
          "roles"
        ]
      },
      "GetSessionAndUserResult": {
        "type": "object",
        "properties": {
          "session": {
            "$ref": "#/components/schemas/IGetSession"
          },
          "user": {
            "$ref": "#/components/schemas/GetSessionUserWithRoles"
          }
        },
        "required": [
          "session",
          "user"
        ]
      }
    }
  }
